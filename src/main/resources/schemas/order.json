{ "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Order",
  "description": "A single order created from requisition",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "externalId": {
      "type": "string",
      "title": "externalId"
    },
    "emergency": {
      "type": "boolean",
      "title": "emergency"
    },
    "facilityId": {
      "type": ["string", "null"],
      "title": "facilityId"
    },
    "processingPeriodId": {
      "type": ["string", "null"],
      "title": "processingPeriodId"
    },
    "createdDate": {
      "type": ["array", "null"],
      "title": "createdDate",
      "items": {
        "type": "number"
      },
      "uniqueItems": false
    },
    "createdById": {
      "type": "string",
      "title": "createdById"
    },
    "programId": {
      "type": "string",
      "title": "programId"
    },
    "requestingFacilityId": {
      "type": "string",
      "title": "requestingFacilityId"
    },
    "receivingFacilityId": {
      "type": "string",
      "title": "receivingFacilityId"
    },
    "supplyingFacilityId": {
      "type": "string",
      "title": "supplyingFacilityId"
    },
    "orderCode": {
      "type": ["string", "null"],
      "title": "orderCode"
    },
    "status": {
      "type": "string",
      "title": "status"
    },
    "quotedCost": {
      "type": "number",
      "title": "quotedCost"
    },
    "orderLineItems": {
      "type": ["array", "null"],
      "title": "orderLineItems",
      "items": {
        "type": "object",
        "$ref": "orderLineItem.json"
      },
      "uniqueItems": false
    },
    "supervisoryNodeId": {
      "type": "string",
      "title": "supervisoryNodeId"
    }
  },
  "required": [
    "id",
    "externalId",
    "emergency",
    "createdById",
    "programId",
    "requestingFacilityId",
    "receivingFacilityId",
    "supplyingFacilityId",
    "status",
    "quotedCost"
  ]
}
