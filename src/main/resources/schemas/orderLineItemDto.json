    },
    "orderable": {
      "type": "object",
      "title": "orderable",
      "$ref": "orderableDto.json"
    },
    "orderedQuantity": {
      "type": "integer",
      "title": "orderedQuantity"
    },
    "filledQuantity": {
      "type": "integer",
      "title": "filledQuantity"
    },
    "approvedQuantity": {
      "type": "integer",
      "title": "approvedQuantity"
    },
    "packsToShip": {
      "type": "integer",
      "title": "packsToShip"
    }
